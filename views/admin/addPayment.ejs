<%- include('../layout/header.ejs') %>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <%- include('../layout/sidebar.ejs') %>
            <div class="content-wrapper">
                <div class="container-xxl flex-grow-1 container-p-y">
                    <h4 class="fw-bold py-3 mb-4">Add Payment Method</h4>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="card mb-4">
                                <% if(typeof message !== 'undefined'){ %>
                                    <h3 class="mt-3 text- property" style="color:green;"><%= message %></h3>
                                   <% } %>
                                <form action="" method="post" enctype="multipart/form-data">
                                    <div class="card-body">
                                        <% if(flash.error.length> 0){ %>
                                            <div class="d-inline-block text-danger fs-5 pb-2" role="alert">
                                                <%= flash.error %>
                                            </div>
                                        <% } %>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="row mb-3">
                                                    <label class="col-sm-2 col-form-label"
                                                        for="basic-icon-default-fullname">Name</label>
                                                    <div class="col-sm-10">
                                                        <div class="input-group input-group-merge">
                                                            <input type="text" class="form-control" name="name" value="" placeholder="Enter Name" pattern="^[A-Za-z0-9 ]+$" required/>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mb-3">
                                                    <label class="col-sm-2 col-form-label"
                                                        for="basic-icon-default-fullname">Key Id</label>
                                                    <div class="col-sm-10">
                                                        <div class="input-group input-group-merge">
                                                            <input type="text" class="form-control" name="key_id" value="" placeholder="Enter Key Id" pattern="^[A-Za-z0-9 ]+$" required/>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mb-3">
                                                    <label class="col-sm-2 col-form-label"
                                                        for="basic-icon-default-fullname">Key Secret</label>
                                                    <div class="col-sm-10">
                                                        <div class="input-group input-group-merge">
                                                            <input type="text" class="form-control" name="key_secret" value="" placeholder="Enter Key Secret" pattern="^[A-Za-z0-9 ]+$" required/>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mb-3">
                                                    <label class="col-sm-2 col-form-label"
                                                        for="basic-icon-default-fullname">Image</label>
                                                    <div class="col-sm-10">
                                                        <div class="input-group input-group-merge">
                                                            <span id="basic-icon-default-fullname2" class="input-group-text">
                                                                <i class="bx bx-image"></i>
                                                            </span>
                                                            <input type="file" id="upload" name="image" class="form-control" accept="image/png, image/jpeg,image/jpg" required/>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mb-3">
                                                    <label class="col-sm-2 col-form-label"
                                                        for="basic-icon-default-fullname">is_enable</label>
                                                        <div class="col-sm-10">
                                                            <div class="form-check form-switch mb-2">
                                                                <input class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" name="is_enable">
                                                            </div>
                                                        </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-11">
                                                <button type="submit" class="btn btn-primary">Submit</button>
                                                <a href="/view-payment-method" class="ms-3 btn btn-dark">Back</a>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            <%- include('../layout/footer.ejs') %>


           
