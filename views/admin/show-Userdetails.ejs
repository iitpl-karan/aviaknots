<%- include('../layout/header.ejs') %>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <%- include('../layout/sidebar.ejs') %>
            <div class="content-wrapper">
                <div class="container-xxl flex-grow-1 container-p-y">
                    <div class="row">
                        <div class="col-md-6">
                            <h4 class="py-3 mb-4">User Details</h4>
                            <div class="card mb-4">
                                <form action="" method="post">
                                    <div class="card-body">
                                        <% if(typeof message !== 'undefined'){ %>
                                            <div class="alert alert-primary alert-dismissible" role="alert"><%= message %>
                                             <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                            </div>
                                         <% } %>
                                         <% if(flash.error.length> 0){ %>
                                            <div class="alert alert-danger alert-dismissible" role="alert">
                                                <%= flash.error %>
                                                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                            </div>
                                        <% } %>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="d-flex mb-3 justify-content-between">
                                                    <label class="col-form-label" for="basic-icon-default-fullname">Name</label>
                                         
                                                        <label class="col-form-label" for="basic-icon-default-fullname"><%= user.username %></label>
                                                </div>
                 
                                                <div class="d-flex mb-3 justify-content-between">
                                                    <label class="col-form-label" for="basic-icon-default-fullname">Email</label>
                                         
                                                    <label class="col-form-label" for="basic-icon-default-fullname"><%= user.email %></label>
                                                </div>
                                

                                                <div class="d-flex mb-3 justify-content-between">
                                                    <label class="col-form-label" for="basic-icon-default-fullname">Number</label>
                                         
                                                        <label class="col-form-label" for="basic-icon-default-fullname"><%= user.phone %></label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-8"></div>
                                            <div class="col-sm-4">
                                                <!-- <button type="submit" class="btn btn-primary">Submit</button> -->
                                                <a href="/dashboard" class="ms-3 btn btn-dark">Back</a>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            <%- include('../layout/footer.ejs') %>


           
